<Window x:Class="GreatSceneEditor.NewScene"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GreatSceneEditor"
        mc:Ignorable="d"
        Title="NewScene" Height="75" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" >
            <TextBlock Text="Введите имя сцены"></TextBlock>
            <TextBox Text="Enter" Name="TBSceneName" GotFocus="TBSceneName_GotFocus" MinWidth="500"></TextBox>

        </StackPanel>
    </Grid>
</Window>
